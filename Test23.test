get-view CFG | click
get-view Results | click
with [get-view "Unit Inspection View"] {
    click
    get-tree | select "r1 := @parameter0: de.visuflow.analyzeMe.ex2.TargetClass2" | get-menu "View in CFG" | click
}
with [get-view Results] {
    click
    with [get-table] {
        get-item "r1 := @parameter0: de.visuflow.analyzeMe.ex2.TargetClass2" -column Unit | check
        get-item "r0.<de.visuflow.analyzeMe.ex2.TargetClass2$Container: de.visuflow.analyzeMe.ex2.TargetClass2 this$0> = r1" 
            -column Unit | check
        get-item "r1 := @parameter0: de.visuflow.analyzeMe.ex2.TargetClass2" -column Unit | uncheck
        get-item "r0.<de.visuflow.analyzeMe.ex2.TargetClass2$Container: de.visuflow.analyzeMe.ex2.TargetClass2 this$0> = r1" 
            -column Unit | uncheck
    }
}
with [get-view Results | get-table] {
    get-item "r0.<de.visuflow.analyzeMe.ex2.TargetClass2$Container: de.visuflow.analyzeMe.ex2.TargetClass2 this$0> = r1" 
        -column Unit | get-property selected | equals false | verify-true
    get-item "r0.<de.visuflow.analyzeMe.ex2.TargetClass2$Container: de.visuflow.analyzeMe.ex2.TargetClass2 this$0> = r1" 
        -column Unit | get-property checked | equals false | verify-true
    get-item "r1 := @parameter0: de.visuflow.analyzeMe.ex2.TargetClass2" -column Unit | get-property selected 
        | equals false | verify-true
    get-item "r1 := @parameter0: de.visuflow.analyzeMe.ex2.TargetClass2" -column Unit | get-property checked 
        | equals false | verify-true
}
with [get-view Results | get-table] {
    get-item "r1 := @parameter0: de.visuflow.analyzeMe.ex2.TargetClass2" -column Unit | check
    get-item "r0.<de.visuflow.analyzeMe.ex2.TargetClass2$Container: de.visuflow.analyzeMe.ex2.TargetClass2 this$0> = r1" 
        -column Unit | check
    select "r1 := @parameter0: de.visuflow.analyzeMe.ex2.TargetClass2" -column Unit
}
get-view Results | get-table | select "r1 := @parameter0: de.visuflow.analyzeMe.ex2.TargetClass2" -column Unit 
    | get-menu "Custom attribute" | click
with [get-window "Set custom attributes"] {
    get-editbox -after [get-label "Attribute:"] | set-text adawd
    get-editbox -after [get-label "Attribute value:"] | set-text adwadwda
    get-button Set | click
}
with [get-view Results | get-table] {
    get-item "r1 := @parameter0: de.visuflow.analyzeMe.ex2.TargetClass2" -column Unit | get-property selected 
        | equals true | verify-true
    get-item "r1 := @parameter0: de.visuflow.analyzeMe.ex2.TargetClass2" -column Unit | get-property checked 
        | equals true | verify-true
}
